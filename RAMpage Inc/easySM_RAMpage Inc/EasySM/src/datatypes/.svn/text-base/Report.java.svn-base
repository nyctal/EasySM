package datatypes;

/**
 *
 * @author davide
 */
public class Report {
    String msg;
    boolean ok;

    public Report(){
        msg="";
        ok=true;
    }

    public Report(String s, boolean o){
        msg=s;
        ok=o;
    }

    public String getMsg() {
        return msg;
    }

    public boolean isOk() {
        return ok;
    }

    public void setMsg(String msg) {
        this.msg = msg;
    }

    public void setOk(boolean ok) {
        this.ok = ok;
    }

    
}
