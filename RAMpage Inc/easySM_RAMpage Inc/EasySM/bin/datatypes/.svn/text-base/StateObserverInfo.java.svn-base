package datatypes;

import store.*;

public class StateObserverInfo extends ElementInfo {

	private Type type;
	private Expr inv;

        public StateObserverInfo(Type t, Expr i, String n){
            setType(t);
            setInv(i);
            setName(n);
        }

        public void setType(Type t){
            type=t;
        }

        public void setInv(Expr i){
            inv=i;
        }

        public Type getType (){
            return type;
        }

        public Expr getInv(){
            return inv;
        }
}