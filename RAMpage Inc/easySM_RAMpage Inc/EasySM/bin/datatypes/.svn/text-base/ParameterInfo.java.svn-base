package datatypes;

import store.*;

public class ParameterInfo extends ElementInfo {

	private Type returnType;
        
        public ParameterInfo(Type t, String n){
            setName(n);
            setReturnType(t);
        }

        public void setReturnType(Type t){
            returnType=t;
        }

        public Type getReturnType(){
            return returnType;
        }

}